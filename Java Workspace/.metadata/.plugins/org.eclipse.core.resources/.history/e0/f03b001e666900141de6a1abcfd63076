package test;

//string based tests
public class StringTests {
	
	
	public StringTests(){
	}
	
	public void splitters(){
		// TODO Auto-generated method stub
		
		String str = "y,n,y,n";
		str = str.replace(":", "").toUpperCase();
		
		char[] strA = str.split(" ");
		
		for(String s: strA){
			System.out.println(s);
		}
	}
}
