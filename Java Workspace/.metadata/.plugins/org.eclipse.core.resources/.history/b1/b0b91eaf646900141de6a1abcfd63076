package test;

public class Testing {
	
	public void splitters(){
		// TODO Auto-generated method stub
		
		String str = "hello: hi";
		str = str.replace(":", "").toUpperCase();
		
		String[] strA = str.split(" ");
		
		for(String s: strA){
			System.out.println(s);
		}
	}
	

	public static void main(String[] args) {
		
		Strings s = new Strings();
		s.splitters();

	}

}
